{
  "name": "clinipro-suite",
  "version": "1.0.0",
  "description": "CliniPro Suite - Sistema Integral de Gestión Clínica",
  "main": "index.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon backend/server.js",
    "client": "npm run build && live-server --port=3000 --entry-file=index.html",
    "build": "echo 'Building frontend...'",
    "test": "jest",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "docker:build": "docker build -t clinipro-suite .",
    "docker:run": "docker run -p 3000:3000 clinipro-suite"
  },
  "keywords": [
    "healthcare",
    "medical",
    "nursing",
    "clinic",
    "telemedicine",
    "pwa"
  ],
  "author": "CliniPro Development Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "socket.io": "^4.7.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.10.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "live-server": "^1.2.2",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
